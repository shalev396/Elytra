{
  "name": "@shalev396/elytra",
  "version": "0.0.1",
  "description": "Elytra is a command-line interface (CLI) tool designed to streamline the initialization of full-stack projects. By prompting users with a series of questions regarding their preferred frontend and backend technologies, Elytra generates a tailored project structure equipped with the necessary configurations and dependencies.",
  "main": "dist/index.js",
  "bin": {
    "elytra": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shalev396/Elytra.git"
  },
  "keywords": [
    "elytra",
    "cli",
    "command-line",
    "project-generator",
    "scaffolding",
    "boilerplate",
    "full-stack",
    "frontend",
    "backend",
    "typescript",
    "javascript",
    "nodejs",
    "npm",
    "init",
    "create-project",
    "project-initializer",
    "starter-kit",
    "template",
    "monorepo",
    "automation"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Shalev Ben Moshe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shalev396/Elytra/issues"
  },
  "homepage": "https://github.com/shalev396/Elytra#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "figlet": "^1.6.0",
    "inquirer": "^8.2.5",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
